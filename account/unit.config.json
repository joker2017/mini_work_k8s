{
  "listeners": {
    "*:8300": {
      "pass": "routes"
    }
  },
  "routes": {
    "main": [
      {
        "match": {
          "uri": "/static/*"
        },
        "action": {
          "share": "/app/static"
        }
      },
      {
        "action": {
          "pass": "applications/account"
        }
      }
    ]
  },
  "applications": {
    "account": {
      "path": "app",
      "module": "app.wsgi",
      "callable": "application"
    }
  }
}