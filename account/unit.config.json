{
  "listeners": {
    "*:8300": {
      "pass": "routes"
    }
  },
  "routes": [

      {
        "match": {
          "uri": "/static/*"
        },
        "action": {
          "share": "/app/app/vol/web/static"
        }
      },
      {
        "action": {
          "pass": "applications/account"
        }
      }
    ],
  "applications": {
    "account": {
      "type": "python",
      "path": "./app",
      "module": "app.wsgi",
      "callable": "application"
    }
  }
}


{
    "listeners": {
        "*:80": {
            "pass": "routes"
        }
    },

    "routes": [
        {
            "match": {
                "uri": "/static/*"
            },

            "action": {
                "share": "/app/static$uri"
            }
        },
        {
            "action": {
                "pass": "applications/django"
            }
        }
    ],

    "applications": {
        "django": {
            "type": "python 3.9",
            "path": "./app",
            "module": "app.wsgi",
            "environment": {
                "DJANGO_SETTINGS_MODULE": "app.settings"
            }
        }
    }
}
