{
  "listeners": {
    "*:8300": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": "/static/*"
      },
      "action": {
        "share": "/vol/web/static"
      }
    },
    {
      "action": {
        "pass": "applications/account"
      }
    }
  ],
  "applications": {
    "profile": {
      "path": "app",
      "module": "app.wsgi",
      "callable": "application"
    }
  }
}