
{
    "listeners": {
        "*:8300": {
            "pass": "routes"
        }
    },

    "routes": [
        {
            "match": {
                "uri": "/static/*"
            },

            "action": {
                "share": "/app/static$uri"
            }
        },
        {
            "action": {
                "pass": "applications/django"
            }
        }
    ],

    "applications": {
        "django": {
            "type": "python 3",
            "processes": 4,
            "threads": 4,
            "path": "/app",
            "module": "app.wsgi"

            }
        }
    }

