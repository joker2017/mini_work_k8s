{
  "listeners": {
    "*:8300": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": "/static/*"
      },
      "action": {
        "share": "/vol/web/static"
      }
    },
    {
      "action": {
        "pass": "applications/app"
      }
    }
  ],
  "applications": {
    "app": {
      "type": "python 3",
      "path": "/app",
      "module": "app.app.wsgi"
    }
  }
}
