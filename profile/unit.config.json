{
  "listeners": {
    "*:8300": {
      "pass": "routes/main"
    }
  },
  "routes": {
    "main": [
      {
        "match": {
          "uri": "/static/*"
        },
        "action": {
          "share": "/app/static"
        }
      },
      {
        "action": {
          "pass": "applications/profile"
        }
      }
    ]
  },
  "applications": {
    "profile": {
      "type": "python",
      "path": "/app",
      "module": "app.wsgi",
      "callable": "application",
      "environment": {
        "DJANGO_SETTINGS_MODULE": "app.settings"
      }
    }
  }
}
